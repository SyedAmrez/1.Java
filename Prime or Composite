import java.util.*;
public class Main
{
	public static void main(String[] args) 
	{
	    int i,x,c=0;
		System.out.print("Enter: ");
		Scanner sc=new Scanner(System.in);
		x=sc.nextInt();
		for(i=1;i<=x;i++)
		{
		    if(x%i==0)
		    {
		        c++;
		    }
		}
		if(c==2)
		{
		    System.out.println("Prime");
		}
		else
		{
		    System.out.println("Composite");
		}
	}
}

