import java.util.Scanner;
public class Main
{
	public static void main(String[] args) 
	{
	    int x,y,sum;
	    Scanner sc = new Scanner(System.in);
	    System.out.print("Enter x= ");
	    x=sc.nextInt();
		System.out.print("Enter y= ");
		y=sc.nextInt();
		sum =x+y;
		System.out.println("The sum = "+sum);
	}
}

