import java.util.*;
public class Main
{
	public static void main(String[] args) 
	{
	    String str,rev="";
	    int i,ln;
	    Scanner sc=new Scanner(System.in);
		System.out.print("Enter word: ");
		str=sc.next();
	    ln=str.length();
	    for(i=ln-1;i>=0;i--)
	    {
	        rev=rev+str.charAt(i);
	    }
	    System.out.print("Reverse: "+rev);
	}
}

